{
  "name": "studio",
  "version": "0.0.1",
  "description": "A template to build 2D games using Pixi.js",
  "main": "index.js",
  "scripts": {
    "build_prod": "webpack --env=production_build",
    "build_test": "webpack --env=development_build --openssl-legacy-provider start",
    "test_game": "node_modules/.bin/webpack-dev-server --env=development_server --openssl-legacy-provider start",
    "prod_game": "node_modules/.bin/webpack-dev-server --env=production_server"
  },
  "author": "Buddhika Jayawickrama, Hashan Chandika",
  "license": "MIT",
  "dependencies": {
    "pixi-sound": "^3.0.4",
    "pixi.js": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.5",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-obfuscator": "^0.18.7"
  }
}
